#include<stdio.h>
int main(){
    int num;
    printf("Entre a number:");
    scanf("%d",&num);
    if(num > 0){
        printf("The number is Positive(+ve).");
    }
    else if(num<0){
        printf("The number is Negative (-ve).");
    }
    else {
        printf("The number is zero.");
    }
    return 0;
}
